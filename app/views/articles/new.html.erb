<h1>New Article</h1>
<%= form_for :article, url: articles_path do |f| %>

<% if @contact && @contact.errors.any?%>
<p>
	<ul>
	<% @contact.errors.full_messages.each { |msg|  } %>
	<li><%= msg %></li>
	</ul>
</p>
<% end %>

<p>

    <%= f.label :title %> <br>
    <%= f.text_field :title %>
</p>
<p>
	<%=f.label :text %> <br>
	<%= f.text_area :text %>
</p>
<p>
	<%= f.submit %>
		
</p>
<% end %>
